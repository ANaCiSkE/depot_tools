MYPATH=$(dirname "${BASH_SOURCE[0]}")

source "$MYPATH/cipd_bin_setup.sh"
cipd_bin_setup

export GIT_EXEC_PATH="$MYPATH/.cipd_bin/libexec/git-core"
export GIT_TEMPLATE_DIR="$MYPATH/.cipd_bin/share/git-core/templates/"
exec "$MYPATH/.cipd_bin/bin/git" "$@"
