[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]/resources/git_util.py",
      "setup",
      "--url",
      "https://chromium.googlesource.com/chromium/src.git",
      "--dir",
      "[START_DIR]/src"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]/resources/git_util.py",
      "--output-json",
      "/path/to/tmp/json",
      "ensure_checkout",
      "--url",
      "https://chromium.googlesource.com/chromium/src.git",
      "--dir",
      "[START_DIR]/src",
      "--recurse",
      "--clean"
    ],
    "name": "git fetch",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "-c",
      "foo=bar",
      "count-objects",
      "-v"
    ],
    "cwd": "[START_DIR]/src",
    "infra_step": true,
    "name": "count-objects",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@exception@ValueError(\"invalid literal for long() with base 10: 'xxx'\",)@@@",
      "@@@STEP_LOG_END@exception@@@",
      "@@@STEP_WARNINGS@@@"
    ]
  },
  {
    "name": "$result",
    "reason": "count-objects failed: invalid literal for long() with base 10: 'xxx'",
    "recipe_result": null,
    "status_code": 1
  }
]