[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]\\bootstrap\\win\\git_bootstrap.py",
      "--verbose"
    ],
    "cwd": "RECIPE_PACKAGE_REPO[depot_tools]",
    "infra_step": true,
    "name": "ensure git tooling on windows",
    "timeout": 300
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]\\resources\\git_util.py",
      "--git-cmd-path",
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "setup",
      "--url",
      "https://chromium.googlesource.com/chromium/src.git",
      "--dir",
      "[START_DIR]\\src"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]\\resources\\git_util.py",
      "--git-cmd-path",
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "--output-json",
      "/path/to/tmp/json",
      "ensure_checkout",
      "--url",
      "https://chromium.googlesource.com/chromium/src.git",
      "--dir",
      "[START_DIR]\\src",
      "--recurse",
      "--clean"
    ],
    "name": "git fetch",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "-c",
      "foo=bar",
      "count-objects",
      "-v"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "count-objects",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "config",
      "--get",
      "remote.origin.url"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git config remote.origin.url",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@foo@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "show",
      "HEAD",
      "--format=%at",
      "-s"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git show",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "fetch",
      "origin",
      "--tags"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git fetch tags"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status can_fail_build"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status cannot_fail_build"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "new-branch",
      "refactor"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];%(PATH)s"
    },
    "infra_step": true,
    "name": "git new-branch refactor"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "new-branch",
      "feature",
      "--upstream",
      "refactor"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];%(PATH)s"
    },
    "infra_step": true,
    "name": "git new-branch feature"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "rebase",
      "origin/master"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "my repo rebase"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "bundle",
      "create",
      "[START_DIR]\\all.bundle",
      "--all"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git bundle"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]