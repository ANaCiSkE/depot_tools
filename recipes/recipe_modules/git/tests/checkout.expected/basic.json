[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]/resources/git_util.py",
      "--git-cache-dir",
      "[CACHE]/git",
      "--output-json",
      "/path/to/tmp/json",
      "ensure_checkout",
      "--url",
      "https://chromium.googlesource.com/chromium/tools/depot_tools.git",
      "--dir",
      "[TMP_BASE]/checkout_tmp_1/checkout",
      "--clean"
    ],
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "rev-parse",
      "HEAD"
    ],
    "cwd": "[TMP_BASE]/checkout_tmp_1/checkout",
    "infra_step": true,
    "name": "git rev-parse HEAD",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::shutil]/resources/rmtree.py",
      "[TMP_BASE]/checkout_tmp_1"
    ],
    "infra_step": true,
    "name": "rmtree [TMP_BASE]/checkout_tmp_1"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]